

def even_odd(n):

    if(n % 2) == 0:
        return True
    elif (n % 2) == 1:
        return False


max = int(input("Enter the number of inputs: "))
inputs = []
for i in range(max):
    x = int(input())
    inputs.append(x)
for i in range(max):
    if even_odd(inputs[i]):
        print(inputs[i], "is even")
    else:
        print(inputs[i], "is odd")
